<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

	<context id="DB2Tables" targetRuntime="MyBatis3Simple" defaultModelType="flat"  >

	    <!-- 根据Mapper生成实体类,xml文件 -->
        <plugin type="tk.mybatis.mapper.generator.MapperPlugin">
            <property name="mappers" value="com.zhichengzhang.ssm.base.MapperUtil"/>
        </plugin>
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin"></plugin>


		<!-- 连接字符串 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
						connectionURL="jdbc:mysql://localhost:3388/crm_service?characterEncoding=UTF-8"
						userId="crm_service"
						password="crm_service">
		</jdbcConnection>

		<!-- 指定生成“entity实体类、mybatis映射xml文件、mapper接口”的具体位置 -->
		 <javaModelGenerator targetPackage="com.zhichengzhang.ssm.model" targetProject="src/main/java" >
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

		<sqlMapGenerator targetPackage="mapping" targetProject="src/main/resources">
			<property name="enableSubPackages" value="true"/>
		</sqlMapGenerator>

		<javaClientGenerator type="XMLMAPPER" targetPackage="com.zhichengzhang.ssm.mapper" targetProject="src/main/java">
			<property name="enableSubPackages" value="true"/>
		</javaClientGenerator>


        <!-- 具体要生成的表，如果有多个表，复制这一段，改下表名即可 -->
		<table tableName="crm_im_permission" domainObjectName="ImPermission"
			   enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"
			   enableSelectByExample="false" selectByExampleQueryId="false">
		</table>

	</context>
</generatorConfiguration>